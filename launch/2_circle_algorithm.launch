<launch>
  <node name="collision_detection" pkg="ros_collision_detection" type="collision_detection" output="screen"/>
  <param name="ttc_algorithm_classname" value="NCircleAlgorithm" type="str" />
  <param name="ttc_algorithm_circle_count" value="2" type="int" />
  <param name="warning_generator_algorithm_classname" value="TTCOnlyWarningAlgorithm" type="str" />
  <param name="subject_vehicle_length" value="5" type="double" />
  <param name="subject_vehicle_width" value="2" type="double" />
</launch>